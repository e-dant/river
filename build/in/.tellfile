- given name
echo river build input directory
- given build
echo ../build
- given exe
echo ../out/`tell workspace`.`tell name`
- given run
echo `tell exe`
- given bun
echo eval `tell build`' && eval '`tell run`
- given clean
echo 'rm -rf ../out/* ../out/.cache ../out/.ninja_* ; unlink ../../compile_commands.json'
